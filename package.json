{
  "name": "@cuberqaq/asuka-ui",
  "version": "1.0.1",
  "description": "UI Framework for Zepp OS",
  "repository": "https://github.com/CuberQAQ/asuka-ui",
  "author": "CuberQAQ",
  "license": "MIT",
  "main": "./dist/index.js",
  "type": "module",
  "types": "./src/index.ts",
  "files": [
